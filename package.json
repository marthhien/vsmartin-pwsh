{
	"name": "vsmartin-pwsh",
	"displayName": "vsmartin-pwsh",
	"publisher": "marthhien",
	"homepage": "https://github.com/marthhien",
	"description": "Integrate vsmartin-pwsh into VS Code",
	"version": "1.0.0",
	"engines": {
		"vscode": "^1.60.0"
	},
	"categories": [
		"Other"
	],
	"icon": "vsmartin-pwsh-icon.png",
	"galleryBanner": {
		"color": "#ACD1EC",
		"theme": "light"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/marthhien/vsmartin-pwsh.git"
	},
	"activationEvents": [
		"*"
	],
	"main": "./src/extension.ts",
	"contributes": {
	"views": {
			"myview": [
				{
					"id": "vsmartin-pwsh-welcome",
					"name": "Extension vsmartin-pwsh"
				}
			]
		},
		"viewsWelcome": [
			{
			  "view": "vsmartin-pwsh-welcome",
			  "contents": "Bienvenue dans vsmartin-pwsh \n [https://github.com/marthhien](https://github.com/marthhien)\n[DÃ©buter](command:vsmartin-pwsh-practice.registerDataProvider)"	
			}
		  ],
		"commands": [
			{
				"command": "vsmartin-pwsh-practice.registerDataProvider",
				"title": "TreeView DataProvider"
			}			
		],
		"viewsContainers": {
			"activitybar": [
				{
					"id": "myview",
					"title": "Extension vsmartin-pwsh",
					"icon": "./vsmartin-pwsh-icon.svg"
				}
			]
		}
	},
	"scripts": {
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./",
		"download-api": "vscode-dts dev",
		"postdownload-api": "vscode-dts main",
		"postinstall": "npm run download-api"
	},
	"devDependencies": {
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.60.0",
		"@typescript-eslint/eslint-plugin": "^4.31.1",
		"@typescript-eslint/parser": "^4.31.1",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^7.32.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.1",
		"tslint": "^5.12.1",
		"vscode": "^1.1.28",
		"typescript": "^4.4.3",
		"vscode-dts": "^0.3.1"
	}
}
